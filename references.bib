@article{Hulbert2004a,
  title = {Basal Metabolic Rate: History, Composition, Regulation, and Usefulness},
  author = {Hulbert, A J and Else, P L},
  year = {2004},
  month = jan,
  journal = {Physiological and Biochemical Zoology},
