  author = {Gi{\l}ka, Wojciech and Zakrzewska, Marta and Lukashevich, Elena D and Vorontsov, Dmitry D and {Soszy{\'n}ska-Maj}, Agnieszka and Skibi{\'n}ska, Kornelia and Cranston, Peter S},
