@article{Andreu-Sanchez2021,
  title = {A Benchmark of Genetic Variant Calling Pipelines Using Metagenomic Short-Read Sequencing},
  author = {{Andreu-S{\'a}nchez}, Sergio and Chen, Lianmin and Wang, Daoming and Augustijn, Hannah E. and Zhernakova, Alexandra and Fu, Jingyuan},
