@article{Bagchi2014a,
  title = {Pathogens and Insect Herbivores Drive Rainforest Plant Diversity and Composition},
  author = {Bagchi, Robert and Gallery, Rachel E and Gripenberg, Sofia and Gurr, Sarah J and Narayan, Lakshmi and Addis, Claire E and Freckleton, Robert P and Lewis, Owen T},
  year = {2014},
  month = jan,
  journal = {Nature},
