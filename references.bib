@article{Holt2011c,
  title = {{{MAKER2}}: An Annotation Pipeline and Genome-Database Management Tool for Second-Generation Genome Projects},
  author = {Holt, Carson and Yandell, Mark},
  year = {2011},
  month = dec,
  journal = {BMC Bioinformatics},
  volume = {12},
