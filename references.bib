  author = {Ives, Anthony R. and Barton, Brandon T. and Penczykowski, Rachel M. and Harmon, Jason P. and Kim, Kyungsun L. and Oliver, Kerry and Radeloff, Volker C.},
  year = {2020},
  journal = {Nature Ecology and Evolution},
  volume = {4},
  number = {5},
  pages = {702--711},
  publisher = {{Springer US}},
  issn = {2397334X},
  doi = {10.1038/s41559-020-1155-0},
  abstract = {Ecological and evolutionary processes may become intertwined when they operate on similar time scales. Here we show ecological\textendash evolutionary dynamics between parasitoids and aphids containing heritable symbionts that confer resistance against parasitism. In a large-scale field experiment, we manipulated the aphid's host plant to create ecological conditions that either favoured or disfavoured the parasitoid. The result was rapid evolutionary divergence of aphid resistance between treatment populations. Consistent with ecological\textendash evolutionary dynamics, the resistant aphid populations then had reduced parasitism and increased population growth rates. We fit a model to quantify costs (reduced intrinsic rates of increase) and benefits of resistance. We also performed genetic assays on 5 years of field samples that showed persistent but highly variable frequencies of aphid clones containing protective symbionts; these patterns were consistent with simulations from the model. Our results show (1) rapid evolution that is intertwined with ecological dynamics and (2) variation in selection that prevents traits from becoming fixed, which together generate self-perpetuating ecological\textendash evolutionary dynamics.},
  pmid = {32203477},
  keywords = {aphids,eco-evo,parasitoid},
  file = {/Users/lucasnell/Zotero/storage/CPSBQH8F/Ives et al._2020_Self-perpetuating ecological–evolutionary dynamics in an agricultural host–parasite system.pdf}
}

