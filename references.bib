@article{Guirao-Rico2021,
  title = {Benchmarking the Performance of {{Pool}}-seq {{SNP}} Callers Using Simulated and Real Sequencing Data},
  author = {Guirao-Rico, Sara and Gonz{\'a}lez, Josefa},
  year = {2021},
