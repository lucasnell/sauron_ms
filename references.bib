@article{Zheng2011,
  title = {Effects of Cadmium Exposure on Lipid Peroxidation and the Antioxidant System in Fourth-Instar Larvae of {{{\emph{Propsilocerus}}}}{\emph{ Akamusi}} ({{Diptera}}: {{Chironomidae}}) under Laboratory Conditions},
  shorttitle = {Effects of Cadmium Exposure on Lipid Peroxidation and the Antioxidant System in Fourth-Instar Larvae of Propsilocerus Akamusi (Diptera},
  author = {Zheng, Xianyun and Long, Wenmin and Guo, Yaping and Ma, Enbo},
