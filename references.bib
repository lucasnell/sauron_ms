@article{Palkovacs2009,
  title = {Experimental Evaluation of Evolution and Coevolution as Agents of Ecosystem Change in {{Trinidadian}} Streams},
  author = {Palkovacs, Eric P. and Marshall, Michael C. and Lamphere, Brad A. and Lynch, Benjamin R. and Weese, Dylan J. and Fraser, Douglas F. and Reznick, David N. and Pringle, Catherine M. and Kinnison, Michael T.},
  year = {2009},
