  author = {Touchon, Marie and Cury, Jean and Yoon, Eun Jeong and Krizova, Lenka and Cerqueira, Gustavo C. and Murphy, Cheryl and Feldgarden, Michael and Wortman, Jennifer and Clermont, Dominique and Lambert, Thierry and {Grillot-Courvalin}, Catherine and Nemec, Alexandr and Courvalin, Patrice and Rocha, Eduardo P.C.},
