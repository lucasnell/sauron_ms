@incollection{Salisbury2000a,
  title = {A Biomechanical Transformation Model for the Evolution of Semi-Spheroidal Articulations between Adjoining Vertebral Bodies in Crocodilians {{BT}}  - {{Crocodilian}} Biology and Evolution},
  booktitle = {Crocodilian Biology and Evolution},
  author = {Salisbury, S W and Frey, E},
  editor = {Grigg, Gordon Clifford and Seebacher, Frank and Franklin, Craig Edwin},
  year = {2000},
  month = jan,
  number = {9},
  pages = {85--134},
  publisher = {{Surrey Beatty \& Sons}},
  address = {{Chipping Norton, NSW, Australia}},
  abstract = {With the exception of the biptanar articulation between the two sacral vertebrae, the adjoining vertebral bodies of extant crocodilians and other eusuchians form synovial, semi-spheroidal articulations. On each of the cervical, thoracic and lumbar vertebrae, the semi-spheroidal-shaped condyle is situated at the caudal end of the vertebral body, while the socket, in which the condyle of the following vertebra articulates, is at the cranial end; a condition that is described as procoelous. The body of the first caudal vertebra is biconvex, having a condyle at both ends. All the other caudal vertebrae are procoelous. As is the case in all crocodilians, both the cranial end of the body of the first sacral vertebra and caudal end of that belonging to the second sacral vertebra are concave. In contrast to eusuchians, the non-sacral vertebrae of most other crocodilians are amphicoelous, meaning that the vertebral body is concave at both ends. Procoelous vertebrae are also known to occur" in a some Cretaceous mesosuchians, namely Bernissartia fagesii, Gilchristosuchus palatinus and at least two species of atoposaurids. Additionally, in S. fagesii the body of the first caudal vertebra is biconvex, whereas in the atoposaurid Theriosuchus pusillus it is convex cranialiy and concave terminally; a condition that is described as opisthocoelous. Despite the systematic significance attached to these features, the reasons for the evolutionary transformation from amphicoelous to either procoelous or opisthocoelous vertebral bodies in crocodilians have never been addressed. A biomechanical model for the axial bracing mechanism of extant crocodilians indicates that during locomotion, the articulations between adjoining vertebral bodies must accommodate dorsoventral and transverse shear loads, together with rostroterminal torsion, compression and shear. In the context of this model, the main mechanical role played by procoelous vertebral bodies is stabilization of these articulations against dorsoventral and transverse shear loads. In the majority of fossil crocodilians with amphicoelous vertebrae, similar models suggest that the influence of dorsoventral and transverse shear loads on the articulations between adjoining vertebral bodies is minimal. This is due primarily to the stabilizing effect of peg-and-groove articulations between adjoining osteoderms of the paravertebral shield. A consequence of this type of bracing mechanism, however, is that lateral flexion of the trunk is considerably restricted, in those crocodilians with amphicoelous vertebrae and a paravertebral shield in which the lateral margins are open, the capacity for lateral flexion is increased, but stabilization of the articulations between adjoining vertebral bodies is problematic unless the animal is of a low mass or fully aquatic. Both procoely and opisthocoely, which most likely involve a more extensive ossification of either the caudal/terminal or cranial end of the vertebral body respectively, represent a mechanical solution to this problem. Similar to many other postcranial features considered characteristic of eusuchians, in the context of a biomechanical model for the axial bracing mechanism, the transformation to semi-spheroidal articulations between adjoining vertebral bodies from an initially biconcave condition can be reconstructed from a number of different fossil crocodilians. This suggests that the current preference for regarding the evolutionary origin of extant crocodilians and other eusuchians as being the result of a single, step-wise transformation process is problematic.},
  isbn = {978-0-949324-89-4},
  keywords = {alligator}
}

