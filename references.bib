  shorttitle = {When Molecules and Morphology Concur},
  author = {Cranston, Peter S. and Hardy, Nate B. and Morse, Geoffrey E. and Puslednik, Louise and McCLUEN, Scott R.},
  year = {2010},
  month = oct,
  journal = {Systematic Entomology},
  volume = {35},
  number = {4},
