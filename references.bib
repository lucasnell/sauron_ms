}

@article{Spottiswoode2011a,
  title = {Ancient Host Specificity within a Single Species of Brood Parasitic Bird},
  author = {Spottiswoode, C N and Stryjewski, K F and Quader, S and {Colebrook-Robjent}, J F R and Sorenson, M D},
  year = {2011},
  month = oct,
  journal = {Proceedings of the National Academy of Sciences of the USA},
