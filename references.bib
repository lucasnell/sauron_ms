@article{Bassar2010,
  title = {Local Adaptation in {{Trinidadian}} Guppies Alters Ecosystem Processes},
  author = {Bassar, R D and Marshall, M C and {Lopez-Sepulcre}, A and Zandona, E and Auer, S K and Travis, J and Pringle, C M and Flecker, A S and Thomas, S A and Fraser, D F and Reznick, D N},
