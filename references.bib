  author = {Reznick, David N. and Bassar, Ronald D. and Handelsman, Corey A. and Ghalambor, Cameron K. and Arendt, Jeff and Coulson, Tim and Potter, Tomos and Ruell, Emily W. and {Torres-Dowdall}, Juli{\'a}n and Bentzen, Paul and Travis, Joseph},
