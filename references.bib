@article{Fry2009c,
  title = {A Central Role for Venom in Predation by {{Varanus}} Komodoensis ({{Komodo Dragon}}) and the Extinct Giant {{Varanus}} ({{Megalania}}) Priscus},
  author = {Fry, Bryan G and Wroe, Stephen and Teeuwisse, Wouter and {van Osch}, Matthias J P and Moreno, Karen and Ingle, Janette and McHenry, Colin and Ferrara, Toni and Clausen, Phillip and Scheib, Holger and Winter, Kelly L and Greisman, Laura and Roelants, Kim and {van der Weerd}, Louise and Clemente, Christofer J and Giannakis, Eleni and Hodgson, Wayne C and Luz, Sonja and Martelli, Paolo and Krishnasamy, Karthiyani and Kochva, Elazar and Kwok, Hang Fai and Scanlon, Denis and Karas, John and Citron, Diane M and Goldstein, Ellie J C and McNaughtan, Judith E and Norman, Janette A},
