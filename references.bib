@article{Guiglielmoni2021,
  title = {Overcoming Uncollapsed Haplotypes in Long-Read Assemblies of Non-Model Organisms},
  author = {Guiglielmoni, Nad{\`e}ge and Houtain, Antoine and Derzelle, Alessandro and Van Doninck, Karine and Flot, Jean Fran{\c c}ois},
  year = {2021},
  journal = {BMC Bioinformatics},
  volume = {22},
