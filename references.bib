  author = {Shaykhutdinov, N.M. and Klink, G.V. and Garushyants, S.K. and Kozlova, O.S. and Cherkasov, A.V. and Kikawada, T. and Okuda, T. and Pemba, D. and Deviatiiarov, R.M. and Gazizova, G.R. and Penin, A.A. and Shagimardanova, E.I. and Cornette, R. and Gusev, O.A. and Bazykin, G.A.},
