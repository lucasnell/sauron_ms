@article{Lenski1991a,
  title = {Long-Term Experimental Evolution in {{Escherichia}} Coli. {{I}}. {{Adaptation}} and Divergence during 2,000 Generations},
  author = {Lenski, Richard E and Rose, Michael R and Simpson, Suzanne C and Tadler, Scott C},
  year = {1991},
