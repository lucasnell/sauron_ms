@article{Barton2007a,
  title = {Herpesvirus Latency Confers Symbiotic Protection from Bacterial Infection},
  author = {Barton, Erik S and White, Douglas W and Cathelyn, Jason S and {Brett-McClellan}, Kelly A and Engle, Michael and Diamond, Michael S and Miller, Virginia L and Virgin IV, Herbert W},
  year = {2007},
