@article{Garud2016a,
  title = {Elevated Linkage Disequilibrium and Signatures of Soft Sweeps Are Common in {{Drosophila}} Melanogaster},
  author = {Garud, Nandita R and Petrov, Dmitri A},
  year = {2016},
