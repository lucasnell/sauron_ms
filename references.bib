@article{Lance2009b,
  title = {Multiyear Multiple Paternity and Mate Fidelity in the {{American}} Alligator, {{Alligator}} Mississippiensis},
  author = {Lance, Stacey L and Tuberville, T D and Dueck, L and {Holz-Schietinger}, C and Trosclair, P L I I I and Elsey, Ruth M and Glenn, T C},
