@article{Vannette2013,
  title = {Nectar Bacteria, but Not Yeast, Weaken a Plant\textendash Pollinator Mutualism},
  author = {Vannette, Rachel L. and Gauthier, Marie Pierre L. and Fukami, Tadashi},
