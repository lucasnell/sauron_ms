publisher = {Elsevier Inc.},
title = {{Circadian variation in plasma-corticosterone in the American alligator, Alligator mississippiensis, and the effects of ACTH injections}},
url = {http://www.sciencedirect.com/science/article/pii/0016648084901928},
volume = {54},
