month = {aug},
number = {8},
pages = {680--688},
title = {{Developmental abnormalities of the gonad and abnormal sex hormone concentrations in juvenile alligators from contaminated and control lakes in Florida.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=7895709{\&}retmode=ref{\&}cmd=prlinks},
volume = {102},
