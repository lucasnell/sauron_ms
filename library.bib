@article{Lance1999a,
abstract = {Ten juvenile alligators, mean body mass 793 g, hatched from artificially incubated eggs and raised under controlled conditions, were held out of water with their jaws held closed for 48 hr. An initial blood sample was taken and further samples collected at 1, 2, 4, 8, 24, and 48 hr. Epinephrine, norepinephrine, and dopamine were measured in plasma aliquots of 1.5 ml using high pressure liquid chromatography with electrochemical detection. Corticosterone was measured by radioimmunoassay. Plasma glucose was measured using the Trinder method and plasma calcium, cholesterol, and triglycerides were measured in an autoanalyzer. Epinephrine was about 4 ng/ml at the initial bleed, but declined steadily to {\textless} 0.4 ng/ml by 24 hr. Norepinephrine was also about 4 ng/ml at the initial bleed, but rose to over 8 ng/ml at 1 hr, and then declined to {\textless} 0.2 ng/ml at 24 hr. A second, but smaller increase in plasma norepinephrine was seen at 48 hr. Plasma dopamine was low at the initial bleed ({\textless} 0.7 ng/ml), rose to over 8 ng/ml at 1 hr, then declined to {\textless} 0.2 ng/ml. Plasma corticosterone rose progressively for the first 4 hr, declined at 8 hr and 24 hr, then rose again at 48 hr. Plasma glucose rose significantly by 24 hr and remained elevated for 48 hr. Plasma calcium increased at 1, 2, and 4 hr then returned to levels not significantly different from the initial sample at 24 and 48 hr. The white blood cells showed changes indicating immune system suppression. By the end of the treatment the hetorophil/lymphocyte ratio increased to 4.7. These results suggest that handling alligators, taking multiple blood samples, and keeping them restrained for more than 8 hr is a severe stress to the animals.},
address = {Center for Reproduction of Endangered Species, Zoological Society of San Diego, California 92112, USA. lvalenti@sunstroke.sdsu.edu},
author = {Lance, Valentine A and Elsey, Ruth M},
file = {:Users/lucasnell/Box Sync/Mendeley/Lance, Elsey{\_}1999{\_}Plasma catecholamines and plasma corticosterone following restraint stress in juvenile alligators.pdf:pdf},
journal = {Journal of Experimental Zoology},
keywords = {alligator,corticosterone,hematology},
month = {may},
number = {6},
pages = {559--565},
title = {{Plasma catecholamines and plasma corticosterone following restraint stress in juvenile alligators}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/(SICI)1097-010X(19990501)283:6{\%}3C559::AID-JEZ7{\%}3E3.0.CO http://2-4/abstract},
