keywords = {mutualism/symbiosis},
month = {may},
number = {5},
pages = {997--1012},
title = {{Phytophagous insect-microbe mutualisms and adaptive evolutionary diversification.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=18298649{\&}retmode=ref{\&}cmd=prlinks},
volume = {62},
year = {2008}
}
