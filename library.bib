file = {:Users/lucasnell/Box Sync/Mendeley/Hamlin et al.{\_}2014{\_}The reproductive hormone cycle of adult female American alligators from a barrier island population.pdf:pdf},
journal = {Reproduction},
keywords = {alligator,hematology,repro hormones},
