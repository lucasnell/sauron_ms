title = {{Global late Quaternary megafauna extinctions linked to humans, not climate change.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=24898370{\&}retmode=ref{\&}cmd=prlinks},
