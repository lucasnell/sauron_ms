author = {Thompson, Andrew R and Nisbet, Roger M and Schmitt, Russell J},
file = {:Users/lucasnell/Box Sync/Mendeley/Thompson, Nisbet, Schmitt{\_}2006{\_}Dynamics of mutualist populations that are demographically open.pdf:pdf},
journal = {Journal of Animal Ecology},
keywords = {models,mutualism/symbiosis},
month = {nov},
number = {6},
pages = {1239--1251},
title = {{Dynamics of mutualist populations that are demographically open}},
url = {http://doi.wiley.com/10.1111/j.1365-2656.2006.01145.x},
volume = {75},
year = {2006}
}
