address = {Department of Biology, University of York, York YO10 5YW, United Kingdom.},
author = {Darby, A C and Douglas, Angela E},
file = {:Users/lucasnell/Box Sync/Mendeley/Darby, Douglas{\_}2003{\_}Elucidation of the transmission patterns of an insect-borne bacterium.pdf:pdf},
journal = {Applied and Environmental Microbiology},
keywords = {aphids,endosymbiosis},
month = {aug},
number = {8},
pages = {4403--4407},
publisher = {American Society for Microbiology},
title = {{Elucidation of the transmission patterns of an insect-borne bacterium.}},
url = {http://aem.asm.org/content/69/8/4403.full},
volume = {69},
year = {2003}
}
