title = {{No Title}},
url = {http://books.google.com/books?id=GbzXe-L8uFgC{\&}printsec=frontcover{\&}dq=intitle:Generalized+Additive+Models+An+Introduction+with+R{\&}hl={\&}cd=1{\&}source=gbs{\_}api},
year = {2006}
