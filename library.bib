pages = {5},
publisher = {BioMed Central},
title = {{Aphid facultative symbionts reduce survival of the predatory lady beetle Hippodamia convergens}},
url = {http://bmcecol.biomedcentral.com/articles/10.1186/1472-6785-14-5},
volume = {14},
year = {2014}
}
