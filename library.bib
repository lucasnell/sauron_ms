month = {nov},
number = {11},
pages = {1894--1907},
publisher = {Cold Spring Harbor Laboratory},
title = {{Efficient identification of Y chromosome sequences in the human and Drosophila genomes}},
url = {http://genome.cshlp.org/cgi/doi/10.1101/gr.156034.113},
volume = {23},
year = {2013}
}
