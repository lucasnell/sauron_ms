pages = {787--792},
publisher = {American Association for the Advancement of Science},
title = {{Gene flow and the geographic structure of natural populations}},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.3576198},
volume = {236},
year = {1987}
}
