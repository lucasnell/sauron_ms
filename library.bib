file = {:Users/lucasnell/Box Sync/Mendeley/Kaiser et al.{\_}2016{\_}The genomic basis of circadian and circalunar timing adaptations in a midge.pdf:pdf},
journal = {Nature},
keywords = {midges,sequenced genome},
month = {dec},
number = {7631},
pages = {69--73},
publisher = {Nature Publishing Group},
title = {{The genomic basis of circadian and circalunar timing adaptations in a midge}},
url = {http://www.nature.com/articles/nature20151},
