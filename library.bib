file = {:Users/lucasnell/Box Sync/Mendeley/Nolte et al.{\_}2013{\_}Genome-wide patterns of natural variation reveal strong selective sweeps and ongoing genomic conflict in Drosophila ma.pdf:pdf},
journal = {Genome Research},
keywords = {Pool-seq},
month = {jan},
