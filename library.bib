file = {:Users/lucasnell/Box Sync/Mendeley/Smith et al.{\_}2007{\_}Annual variation in the benefits of a nesting association between red phalaropes (Phalaropus fulicarius) and Sabine's.pdf:pdf},
journal = {The Auk},
keywords = {nest protection},
month = {jan},
number = {1},
pages = {276--290},
title = {{Annual variation in the benefits of a nesting association between red phalaropes (Phalaropus fulicarius) and Sabine's gulls (Xema sabini)}},
url = {http://www.bioone.org/perlserv/?request=get-document{\&}doi=10.1642{\%}2F0004-8038{\%}282007{\%}29124{\%}5B276{\%}3AAVITBO{\%}5D2.0.CO{\%}3B2},
