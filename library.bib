journal = {Proceedings of the National Academy of Sciences of the USA},
keywords = {genome evolution,speciation},
month = {may},
number = {21},
pages = {9724--9729},
publisher = {National Acad Sciences},
title = {{Widespread genomic divergence during sympatric speciation}},
