author = {Moran, Nancy A and Jarvik, Tyler},
file = {:Users/lucasnell/Box Sync/Mendeley/Moran, Jarvik{\_}2010{\_}Lateral transfer of genes from fungi underlies carotenoid production in aphids.pdf:pdf},
journal = {Science},
keywords = {aphids},
