file = {:Users/lucasnell/Box Sync/Mendeley/Nagy{\_}2005{\_}Field metabolic rate and body size.pdf:pdf},
journal = {Journal of Experimental Biology},
keywords = {alligator,energetics/metabolism},
month = {may},
number = {9},
pages = {1621--1625},
title = {{Field metabolic rate and body size}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=15855393{\&}retmode=ref{\&}cmd=prlinks},
volume = {208},
year = {2005}
}
