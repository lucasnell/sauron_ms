publisher = {Cold Spring Harbor Laboratory},
title = {{Genome-wide patterns of natural variation reveal strong selective sweeps and ongoing genomic conflict in Drosophila mauritiana}},
url = {http://genome.cshlp.org/cgi/doi/10.1101/gr.139873.112},
volume = {23},
year = {2013}
}
