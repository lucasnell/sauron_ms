pages = {289--304},
publisher = {Genetics},
title = {{Genetic dissection of a key reproductive barrier between nascent species of house mice}},
url = {http://www.genetics.org/cgi/doi/10.1534/genetics.111.129171},
volume = {189},
year = {2011}
}
