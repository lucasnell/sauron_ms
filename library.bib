author = {Garud, Nandita R and Petrov, Dmitri A},
file = {:Users/lucasnell/Box Sync/Mendeley/Garud, Petrov{\_}2016{\_}Elevated linkage disequilibrium and signatures of soft sweeps are common in Drosophila melanogaster.pdf:pdf},
journal = {Genetics},
